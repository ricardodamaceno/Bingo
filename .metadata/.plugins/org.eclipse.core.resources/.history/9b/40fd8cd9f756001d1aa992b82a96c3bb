package modelo;

import java.util.ArrayList;
import java.util.List;

public class Cartelas {
	
	private int numeroSerie;
	private int [] numerosCartela;
    	
	public Cartelas(int numerosCartela) {
		this.numerosCartela = 1;
	}
	public int getNumeroSerie() {
		return numeroSerie;
	}
	public void setNumeroSerie(int numeroSerie) {
		this.numeroSerie = numeroSerie;
	}
	public int getNumerosCartela() {
		return numerosCartela;
	}
	public void setNumerosCartela(int numerosCartela) {
		this.numerosCartela = numerosCartela;
	}
	
	
	
	

}
